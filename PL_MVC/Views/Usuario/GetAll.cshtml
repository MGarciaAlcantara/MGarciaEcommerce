@model ML.Usuario
@{
    ViewBag.Title = "GetAll";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>GetAll</h2>
<div class="container">
    <div class="row">
        @using (Html.BeginForm("GetAll", "Usuario", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {

            <div class="col-md-3">
                @Html.TextBoxFor(model => model.Nombre, new { @class = "form-control", @placeholder = "Nombre" })
                @Html.ValidationMessageFor(model => model.Nombre)
            </div>
            <div class="col-md-3">
                @Html.TextBoxFor(model => model.ApellidoPaterno, new { @class = "form-control", @placeholder = "Apellido Paterno" })
                @Html.ValidationMessageFor(model => model.ApellidoPaterno)
            </div>
            <div class="col-md-3">
                @Html.TextBoxFor(model => model.ApellidoMaterno, new { @class = "form-control", @placeholder = "Apellido Materno" })
                @Html.ValidationMessageFor(model => model.ApellidoMaterno)
            </div>
            <input type="submit" value="Buscar" id="btnSubmit" class="btn btn-success" onclick="refresh" />
            @*<button onclick="window.location.href=window.location.href">Refresh Page</button>*@

        }
    </div>
</div>
<div class="container ">
    <div class="row">
        <br />

    </div>
</div>
<div class="container ">
    <div class="row">
        <br />
        @using (Html.BeginForm("CargaDatos", "Usuario", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <input type="file" name="archivo" />
            <br />
            <input type="submit" value="Cargar" class="btn btn-primary" />

            <br />
            <b>@ViewBag.mensaje</b>

        }
        @*<input type="file" name="FileUpload" />
        <br />
        <input type="submit" value="Cargar Datos" class="btn btn-primary" />*@

        <center>
            <div style="float:right; margin-bottom:1rem; ">
                @Html.ActionLink("Agregar Usuario ", "Form", "Usuario", htmlAttributes: new { @class = "btn btn-primary" })
            </div>
        </center>
        <table class="table table-bordered table-responsive col-md-10">
            <thead>
                <tr>
                    <td>Editar</td>
                    <td class="hidden">Idusuario</td>
                    <td>Email</td>
                    <td>Password</td>
                    <td>Nombre</td>
                    <td>ApellidoPaterno</td>
                    <td>Apellidomaterno</td>
                    <td>FechaNacimiento</td>
                    <td>Sexo</td>
                    <td> Status</td>
                    <td>Direccion</td>
                    <td>Rol</td>
                    <td>Telefono</td>

                </tr>
            </thead>
            <tbody>
                @foreach (ML.Usuario usuario in Model.Usuarios)
                {
                    <tr>
                        <td><a class="btn btn-primary glyphicon glyphicon-edit text-center" href="@Url.Action("Form", "Usuario", new { IdUsuario=usuario.IdUsuario})"></a></td>
                        <td class="hidden">Idusuario</td>
                        <td>@usuario.Email</td>
                        <td>@usuario.UsuarioPassword</td>
                        <td>@usuario.Nombre</td>
                        <td>@usuario.ApellidoPaterno</td>
                        <td>@usuario.ApellidoMaterno</td>
                        <td>@usuario.FechaNacimiento.ToShortDateString()</td>
                        <td>@usuario.Sexo</td>
                        @*<td>@usuario.UsuarioStatus</td>*@
                        @if (usuario.UsuarioStatus == true)
                        {
                            <td><input type="image" src="data:image/jpeg;base64," style=" height:60px; width:90px;" onclick="location.href='@Url.Action("UpdateStatus", "Usuario",  new { IdUsuario = usuario.IdUsuario })'" /></td>
                            @*<td>       <a href=@Url.Action("UpdateStatus", "Usuario", new { IdUsuario = usuario.IdUsuario }) class="btn btn-primary" style="font-size:medium; align-content:center;  ">
                                <img src="../Models/ios-toggle.jpg"></a></td>*@

                        }
                        else
                        {
                            @*<td>       <a href=@Url.Action("UpdateStatus", "Usuario", new { IdUsuario = usuario.IdUsuario }) class="btn btn-primary" style="font-size:medium; align-content:center;  ">
                                <img src="../Models/CSS3-Only-iOS-Switch-Button.png"></a></td>*@
                            <td><input type="image" src="data:image/jpeg;base64," style=" height:60px; width:90px;" onclick="location.href='@Url.Action("UpdateStatus", "Usuario", new { IdUsuario = usuario.IdUsuario})'" /></td>

                        }

                        <td>@usuario.IdDireccion</td>
                        <td>@usuario.Rol.Nombre</td>
                        <td>@usuario.Telefono</td>

                        @*<td><a class="btn btn-success glyphicon glyphicon-trash text-center" href="@Url.Action("Delete", "Producto", new { IdUsuario=usuario.IdUsuario})" onclick="return confirm('Estas seguro que deseas eliminar este registro?');"></a></td>*@

                    </tr>
                }
            </tbody>
        </table>
    </div>

    <div>

    </div>

</div>
