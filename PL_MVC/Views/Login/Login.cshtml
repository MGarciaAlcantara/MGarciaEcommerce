@model ML.Usuario
@Html.Raw(TempData["msg"])
@{
    Layout = null;
}

 @ViewData["msg"];
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Iniciar Sesion</title>
    <link href="~/content/LogIn.css" rel="stylesheet" />




</head>
<body>
    <div class="row">
        <div class="col-md-12">

            @using (Html.BeginForm("Login", "Login", FormMethod.Post))
            {

                <div class="login-page">
                    <div class="form">

                        <form class="login-form">
                            @Html.TextBoxFor(model => model.Email, new { @class = "form-control", placeholder = "Email", type = "text" })
                            @Html.TextBoxFor(model => model.UsuarioPassword, new { @class = "form-control", placeholder = "Password", type = "password" })

                            <input type="submit" value="iniciar sesion" />

                        </form>
                    </div>
                </div>
            }
        </div>
    </div>
</body>

</html>
